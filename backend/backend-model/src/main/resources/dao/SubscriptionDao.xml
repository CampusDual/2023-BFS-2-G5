<?xml version="1.0" encoding="UTF-8"?>
<JdbcEntitySetup xmlns="http://www.ontimize.com/schema/jdbc"
                 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                 xsi:schemaLocation="http://www.ontimize.com/schema/jdbc http://www.ontimize.com/schema/jdbc/ontimize-jdbc-dao.xsd"
                 catalog="" table="SUBSCRIPTION" datasource="mainDataSource" sqlhandler="dbSQLStatementHandler">
    <DeleteKeys>
        <Column>SUBS_ID</Column>
    </DeleteKeys>
    <UpdateKeys>
        <Column>SUBS_ID</Column>
    </UpdateKeys>
    <GeneratedKey>subs_id</GeneratedKey>

    <Queries>
        <Query id="default">
            <AmbiguousColumns>
                <AmbiguousColumn name="PLATF_ID" prefix="P"/>
                <AmbiguousColumn name="FR_ID" prefix="F"/>
            </AmbiguousColumns>
            <Sentence>
                <![CDATA[
                     SELECT
                      #COLUMNS#
                     FROM
                      PUBLIC.SUBSCRIPTION S
                     INNER JOIN PUBLIC.PLATFORM P ON
                      S.PLATF_ID = P.PLATF_ID
                     INNER JOIN PUBLIC.FREQUENCY F ON
                      S.FR_ID = F.FR_ID
                      #WHERE#
                   ]]>
            </Sentence>
        </Query>
    </Queries>
</JdbcEntitySetup>